{
  "name": "stabilize.js",
  "version": "0.0.1",
  "description": "Small util for creating immutable data",
  "homepage": "https://www.npmjs.com/package/stabilize.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/stabilize.js"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
